{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "quotes": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-var": 1,
    "one-var": 0,
    "prefer-const": 1,
    "semi": [1, "always"],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-new-object": 2,
    "object-shorthand": 1,
    "quote-props": 0,
    "no-array-constructor": 2,
    "no-useless-escape": 1,
    "prefer-rest-params": 1,
    "no-param-reassign": 2,
    "prefer-arrow-callback": 1,
    "arrow-spacing": 1,
    "arrow-parens": 0,
    "arrow-body-style": 1,
    "dot-notation": 2,
    "eqeqeq": 1,
    "no-nested-ternary": 1,
    "no-unneeded-ternary": 1,
    "brace-style": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "space-before-blocks": 1,
    "keyword-spacing": 1,
    "space-infix-ops": 1,
    "newline-per-chained-call": 2,
    "no-whitespace-before-property": 2,
    "space-in-parens": 1,
    "array-bracket-spacing": 1,
    "object-curly-spacing": 1,
    "max-len": [1, 100],
    "comma-style": 1
  },
  "globals": {
  }
}